<%- include('./partials/defaults'); -%>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href="https://use.fontawesome.com/releases/v5.0.4/css/all.css" rel="stylesheet">
    <link rel="stylesheet" href="/dashboard/dash2.css">
    <title>DashBoard</title>
</head>

<body>
    

    <%- include('./partials/navbar') -%>
    <img src="ringing(1).jpeg" alt="" class="notification-icon" id="notification-icon" onclick="opennotimenu()">
    <div class = "notification-menu" id="notification-menu">
        <img src="./photos/cross.png" class="close-noti" id="close-noti" onclick="closenotimenu()">
        <div class="notification-button">
            <p>@jatt69 want to be your friend !</p>
            <div class="acc-rej">
                <button class="friend-request" id="friend-request-noti" onclick="opennotitable()">Accept</button>
                <button class="split-request">Decline</button>
            </div>
            <p>@diljit100 want to be your friend !</p>
            <div class="acc-rej">
                <button class="friend-request" id="friend-request-noti" onclick="opennotitable()">Accept</button>
                <button class="split-request">Decline</button>
            </div>
            <p>@mf609 want to be your friend !</p>
            <div class="acc-rej">
                <button class="friend-request" id="friend-request-noti" onclick="opennotitable()">Accept</button>
                <button class="split-request">Decline</button>
            </div>
            <p>@tilak_atri requested for bill split , Your amount = 500</p>
            <div class="acc-rej">
                <button class="friend-request" id="friend-request-noti" onclick="opennotitable()">Pay Now</button>
                <button class="split-request">Not Now</button>
            </div>
        </div>
    </div>
    <main>
    
        <section class="welcome">
            <div class="welcome-text ">
                <p>â€‹Welcome to <span>Easy Bills</span>! like<br> name suggests, Bill<br> <span>Splitting</span> made easy,but<br> wait we got
                    more!!</p>
            </div>
            <div class="welcome-img"><img src="/dashboard/photos/4142132.jpg" alt=""></div>
            </div>
        </section>
        <section class="feature">
                <img src="/dashboard/photos/2784130-removebg-preview.png" class="" alt="">
            <div class="feature-button">
                    <button target="#" onclick="window.location.href='splitbill'" class="split-bill">Split Bill</button>
                    <button target="#" onclick="window.location.href='Error404'" class="restaurant">Restaurant</button>
                    <button target="#" onclick="window.location.href='add-friend'" class="add-friend">Add Friend</button>
            </div>
        </section>
        <section class="function">
            <div class="fun-icons">
                <a href="/dashboard/all-friend"><div class="fun-icon-1">
                    <img src="/dashboard/photos/add-friend.png" alt="">
                    <p>Search Friend</p>
                </div></a>
                <a href="/wallet"><div class="fun-icon-2">
                    <img src="/dashboard/photos/payment-method.png" alt="">
                    <p>Wallet</p>
                </div></a>
                <a href="/dashboard/Error404"><div class="fun-icon-3">
                    <img src="/dashboard/photos/qr-code.png" alt="">
                    <p>Scan QR</p>
                </div></a>
                <a href="/dashboard/splitbill">
                    <div class="fun-icon-4">
                        <img src="/dashboard/photos/divided.png" alt="">
                        <p>Split Bills
                        </p>
                    </div>
                </a>
                <a href="/dashboard/Error404">
                    <div class="fun-icon-5">
                        <img src="/dashboard/photos/clock.png" alt="">
                        <p>History
                        </p>
                    </div>
                </a>
            </div>
        </section>


        <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
                    aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                    aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                    aria-label="Slide 3"></button>
            </div>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="/dashboard/photos/nick-karvounis-Ciqxn7FE4vE-unsplash.jpg" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>The Yellow Chilli</h5>
                        <p>Some representative placeholder content for the first slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="/dashboard/photos/pablo-merchan-montes-Orz90t6o0e4-unsplash.jpg" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Barbeque Nation</h5>
                        <p>Some representative placeholder content for the second slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="/dashboard/photos/patrick-tomasso-GXXYkSwndP4-unsplash.jpg" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Draught Beer Cafe</h5>
                        <p>Some representative placeholder content for the third slide.</p>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </main>
    <%- include('partials/footer') -%>

    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"
        integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
        integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF"
        crossorigin="anonymous"></script>
    <script src="/dashboard/dash.js"></script>
    <script src="/dashboard/dash2.js"></script>

</body>

</html>